{
  "name": "benblank.github.io",
  "version": "0.0.0",
  "author": "Ben \"535\" Blank",
  "license": "BSD-3-Clause",
  "private": true,
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/benblank/benblank.github.io.git"
  },
  "engines": {
    "node": ">=20.6.0"
  },
  "scripts": {
    "build": "node --import=./tsc-register.js node_modules/@11ty/eleventy/cmd.cjs --config=eleventy.config.ts",
    "ci": "npm-run-all --parallel --silent ci:*",
    "ci:prettier": "prettier --check .",
    "ci:types": "tsc",
    "clean": "rimraf _site",
    "serve": "node --import=./tsc-register.js node_modules/@11ty/eleventy/cmd.cjs --config=eleventy.config.ts --incremental --serve",
    "watch": "node --import=./tsc-register.js node_modules/@11ty/eleventy/cmd.cjs --config=eleventy.config.ts --incremental --watch"
  },
  "prettier": {
    "proseWrap": "always"
  },
  "devDependencies": {
    "@11ty/eleventy": "3.0.0-alpha.3",
    "@sindresorhus/slugify": "^2.2.1",
    "@tsconfig/node20": "^20.1.2",
    "@tsconfig/strictest": "^2.0.2",
    "@types/markdown-it": "^13.0.7",
    "@types/markdown-it-container": "^2.0.9",
    "@types/markdown-it-footnote": "^3.0.3",
    "@types/node": "^20.10.0",
    "@types/vhtml": "^2.2.9",
    "markdown-it-abbr": "^1.0.4",
    "markdown-it-anchor": "^8.6.7",
    "markdown-it-container": "^3.0.0",
    "markdown-it-footnote": "^3.0.3",
    "markdown-it-highlightjs": "^4.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.1.0",
    "rimraf": "^5.0.5",
    "typescript": "^5.3.2",
    "vhtml": "^2.2.0"
  }
}
